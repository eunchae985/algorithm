/* 피보나치 */

class Solution {
  public long solution(int n) {
      long answer = 0;
      long one = 1;
      long two = 1;
      
      if(n==1){
        answer = 1;
      }else{
          for(int i=0; i<n-1; i++){
              answer = (one+two)%1234567;    //long형이라 그런가 one+two 더하고 바로 1234657나눠줘야함. for문 밖에서 더해주면 안됨
                                            //아직 이해안가는 부분이니 찾아보기
              one = two;
              two = answer;
          }
      }
      
      return answer;
      
      /*long answer = 1;
      int two_cnt = 0; //두칸 뛸 수 있는 카운트
      
      two_cnt = n/2;
      
      for(int i=1; i<=two_cnt; i++){
          if(n-(i*2) == 0){
              answer++;
          }else{
            answer = answer + n-(i*2)+i;
          }
      }*/
  }
}
